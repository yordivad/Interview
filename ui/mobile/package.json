    "react-native": "0.61.5",
    "react-native-http": "^0.0.3",
    "reason-react": "^0.7.0",
    "reason-react-native": "^0.61.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^0.0.7",
    "babel-jest": "^25.2.6",
    "bs-platform": "^7.2.2",
    "eslint": "^6.8.0",
    "jest": "^25.2.7",
    "metro-react-native-babel-preset": "^0.66.1",
    "react-test-renderer": "16.13.0"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/tests/**/*.[j]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
